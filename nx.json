{
  "npmScope": "flowteur",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*",
    ".eslintrc.json": "*"
  },
  "projects": {
    "flowteur": {
      "tags": []
    },
    "flowteur-e2e": {
      "tags": [],
      "implicitDependencies": [
        "flowteur"
      ]
    },
    "media-api": {
      "tags": []
    },
    "anchor": {
      "tags": []
    },
    "medias-page": {
      "tags": []
    },
    "video-page": {
      "tags": []
    },
    "api-model": {
      "tags": []
    },
    "flowteur-ssr": {
      "tags": []
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "flowteur",
  "generators": {
    "@schematics/angular:component": {
      "prefix": "flw",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "flw"
    }
  }
}
